nome = 'Luis henrique'
idade = 22  # int
altura = 1.63  # float
e_maior = idade > 18  # bool
peso = 60
imc = idade / (altura ** 2)

print(nome, 'tem', idade, 'anos de idade e seu imc e', imc)
print(f'{nome} tem {idade} anos de idade e seu imc e {imc:.2f}')
print('{} tem {} anos de idade e seu imc e {:.2f}'.format(nome, idade, imc))
